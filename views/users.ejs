<% include partials/header %>

<div class="row">
  <table class="col-md-6 table table-striped">
    <tr>
      <th>Username</th>
      <th>email</th>
    </tr>

  <% users.forEach(function(user) {%>
<tr>
    <td><a href="/users/<%=user._id%>"><%= user.displayName %></td>
    <td><%= user.email %></td>
</tr>
<% }) %>
</table>
</div>

<% include partials/footer %>
